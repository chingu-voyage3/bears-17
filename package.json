  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "npm-run-all -pnr dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon -r dotenv/config ./server/index.js",
    "start": "node -r dotenv/config ./server/index.js",
    "test": "cross-env NODE_ENV=test jest --watchAll"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^App$": "<rootDir>/client/App.jsx",
      "^Components/(.*)$": "<rootDir>/client/components/$1.jsx",
      "^Containers/(.*)$": "<rootDir>/client/containers/$1.jsx",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/client/tests/__mocks__/fileMock.js"
    }
  },
